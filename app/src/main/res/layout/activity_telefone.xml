<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="phone"
            android:ems="10"
            android:id="@+id/telefone" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.572"
            tools:text="Telefone"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="phone"
            android:ems="10"
            android:id="@+id/celular"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintVertical_bias="0.376"
            android:textColor="#3A3939" android:textSize="18sp" tools:text="celular"/>
    <TextView
            android:text="Celular"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/txtCelular"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.265"
            app:layout_constraintVertical_bias="0.924" app:layout_constraintBottom_toTopOf="@+id/celular"/>
    <TextView
            android:text="Telefone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/txtTelefone"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.278"
            app:layout_constraintTop_toBottomOf="@+id/celular" app:layout_constraintBottom_toTopOf="@+id/telefone"
            app:layout_constraintVertical_bias="0.746"/>

</androidx.constraintlayout.widget.ConstraintLayout>